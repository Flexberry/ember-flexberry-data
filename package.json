{
  "name": "ember-flexberry-data",
  "version": "3.8.0-beta.0",
  "description": "Support of database projections, JavaScript Query Language and working with several kinds of backends",
  "keywords": [
    "ember-addon",
    "flexberry"
  ],
  "license": "MIT",
  "author": {
    "name": "Flexberry Team",
    "email": "mail@flexberry.net",
    "url": "http://flexberry.net/"
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Flexberry/ember-flexberry-data.git"
  },
  "scripts": {
    "build": "ember build",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --no-ignore",
    "*.hbs": "ember-template-lint --config '{ \"extends\": [\"recommended\", \"stylistic\"], \"rules\": { \"no-bare-strings\": true } }'"
  },
  "dependencies": {
    "ember-cli-babel": "^6.6.0",
    "ember-cli-htmlbars": "^2.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^2.4.5",
    "dexie": "^2.0.2",
    "ember-ajax": "^3.0.0",
    "ember-browserify": "1.1.9",
    "ember-cli": "~3.1.3",
    "ember-cli-dependency-checker": "^2.0.0",
    "ember-cli-deprecation-workflow": "^0.2.4",
    "ember-cli-htmlbars-inline-precompile": "^1.0.0",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-moment-shim": "^3.6.0",
    "ember-cli-qunit": "^4.1.1",
    "ember-cli-release": "0.2.9",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^2.0.0",
    "ember-data": "~3.1.1",
    "ember-disable-prototype-extensions": "^1.1.2",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-moment": "7.6.0",
    "ember-resolver": "^4.0.0",
    "ember-source": "~3.1.0",
    "ember-source-channel-url": "^1.0.1",
    "ember-template-lint": "^2.18.0",
    "ember-try": "^0.2.23",
    "ember-welcome-page": "^3.0.0",
    "eslint": "^7.18.0",
    "eslint-plugin-ember": "^10.1.2",
    "eslint-plugin-jsdoc": "^31.4.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-qunit": "^5.3.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "loader.js": "^4.2.3",
    "node-uuid": "^1.4.7",
    "npm-run-all": "^4.1.5"
  },
  "engines": {
    "node": "^4.5 || 6.* || >= 7.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
